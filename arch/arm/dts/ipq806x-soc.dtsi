				base = <0x37200000>;
				unit = <1>;
				is_macsec = <1>;
				mac_pwr0 = <0x00080000>;
				mac_pwr1 = <0x00040000>;
				mac_conn_to_phy = <0>;
				phy_interface_type = <7>;
				phy_address_count = <1>;
