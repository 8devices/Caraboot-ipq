			gmac2_cfg {
				base = <0x37400000>;
				unit = <2>;
				is_macsec = <1>;
				mac_pwr0 = <0x00080000>;
				mac_pwr1 = <0x00040000>;
				mac_conn_to_phy = <0>;
				phy_interface_type = <2>;
				phy_address_count = <4>;
				phy_address = <0 1 2 3>;
				phy_name = "IPQ MDIO2";
			};
		};
	};

